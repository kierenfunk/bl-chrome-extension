{
  "name": "Broker Labz",
  "description": "Create Broker Labz reports straight from the browser",
  "version": "1.0",
  "manifest_version": 3,
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "content_scripts": [{
    "js": ["/js/sidebar.js"],
    "matches": [ "*://*.connective.com.au/*" ],
    "exclude_matches": [ "*://login.connective.com.au/*", "*://auth.connective.com.au/*" ],
    "run_at": "document_idle"
  },
  {
    "js": ["/js/contentScript.js"],
    "matches": [ "*://commissions.connective.com.au/*" ],
    "run_at": "document_idle"
  },
  {
    "js": ["/js/crmContentScript.js"],
    "matches": [ "*://crm.connective.com.au/*" ],
    "run_at": "document_idle"
  }],
  "web_accessible_resources": [
    {
      "resources": [ "/js/inject.js" ],
      "matches": [ "*://commissions.connective.com.au/*", "*://crm.connective.com.au/*" ]
    }
  ],
  "permissions": [
    "storage"
  ]
}